{
  "name": "hyeonjin",
  "version": "1.0.0",
  "description": "Hyeonjin's portfolio",
  "dependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.7",
    "autoprefixer": "^9.7.3",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.0",
    "mini-css-extract-plugin": "^0.9.0",
    "morgan": "^1.9.1",
    "node-sass": "^4.13.0",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "postcss": "^7.0.25",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "pug": "^3.0.1",
    "sass-loader": "^8.0.0",
    "scss-loader": "0.0.1",
    "webpack": "^4.41.4",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "gh-pages": "^2.2.0",
    "nodemon": "^2.0.2"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node init.js --delay 2",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "copy -R src/static build && copy -R src/views build",
    "build": "npm run build:server && npm run build:assets && copyAll",
    "prebuild": "rm -rf build",
    "deploy": "gh-pages -d build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/humonnom/hyeonjin.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/humonnom/hyeonjin/issues"
  },
  "homepage": "https://hyunahoh.github.io/hj_cv/"
}
